<div class="player d-flex row my-1 align-items-center col-xs-6">
  <% if position == 1 %>
    <img src="<%= "https://fantasy.premierleague.com/dist/img/shirts/standard/shirt_#{player[0].shirt}_1-110.webp" %>" alt="" style="width: 20%; height: 20%; padding-right: 0px" class="col-md-3">
  <% else %>
    <img src="<%= "https://fantasy.premierleague.com/dist/img/shirts/standard/shirt_#{player[0].shirt}-110.webp" %>" alt="" style="width: 20%; height: 20%; padding-right: 0px" class="col-md-3">
  <% end %>
  <div class="stat column col-8" style="text-align: left; padding-right: 0px;">
    <p style="font-size: small; margin-bottom: 0px"><strong><%= player[0].web_name %></strong>, <%= player[1] %>%</p>
  </div>
</div>
