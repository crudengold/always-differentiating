<% positions = ["ignore", "GKs", "Defs", "Mids", "Fwds"] %>
<div class="row m-1">
  <% (1..4).each do |i| %>
    <div class="players column col-xs-6 col-sm-3 m-1">
      <h4><strong><%= positions[i] %></strong></h4>
      <%= render 'position', position: i, percentage: 15, illegal_players: illegal_players %>
    </div>
  <% end %>
</div>
