<div class="column" style="text-align: center">
  <div>
    <h4>Numbers for Gameweek <%= @gameweek %></h4>
    <h6>Updated at <%= @update_time %></h6>
  </div>

  <div class="main-box col-xs-12 col-sm-12">

    <h1 style="text-align: center"><strong>Illegal players</strong></h1>
    <div>
      <h2 style="text-align: center">Illegal to own:</h2>
        <h3>GKs</h3>
        <div class="players row">
        <% @illegal_players.each do |stat| %>
          <% if stat.player.element_type == 1 %>
            <% if stat.selected_by >= 15 %>
            <div class="player">
              <img src="<%= "https://fantasy.premierleague.com/dist/img/shirts/standard/shirt_#{stat.player.shirt}-110.webp" %>" alt="" style="width: 30%">
              <div class="stat" style="align: center">
                <h6><strong><%= stat.player.web_name %></strong></h6>
                <p><%= stat.selected_by %>%</p>
              </div>
            </div>
            <% end %>
          <% end %>
        <% end %>
        </div>
        <h3>Defs</h3>
        <div class="players row">
        <% @illegal_players.each do |stat| %>
          <% if stat.player.element_type == 2 %>
            <% if stat.selected_by >= 15 %>
            <div class="player">
              <img src="<%= "https://fantasy.premierleague.com/dist/img/shirts/standard/shirt_#{stat.player.shirt}-110.webp" %>" alt="" style="width: 30%">
              <div class="stat" style="align: center">
                <h6><strong><%= stat.player.web_name %></strong></h6>
                <p><%= stat.selected_by %>%</p>
              </div>
            </div>
            <% end %>
          <% end %>
        <% end %>
        </div>
        <h3>Mids</h3>
