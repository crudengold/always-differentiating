                <p><%= stat.selected_by %>%</p>
              </div>
            </div>
            <% end %>
          <% end %>
        <% end %>
        </div>
    </div>
    <div>
      <h2 style="text-align: center">Illegal to buy:</h2>
        <h3>GKs</h3>
        <div class="players row">
        <% @illegal_players.each do |stat| %>
