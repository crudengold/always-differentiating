<div class="player d-flex row my-1 align-items-center col-xs-6">
  <%= image_tag "#{shirt_image}", alt:"", style:"width: 20%; height: 20%; padding-right: 0px", class:"col-md-3" %>
  <div class="stat column col-8" style="text-align: left; padding-right: 0px;">
    <p id="player-stats" style="font-size: 85%; margin-bottom: 0px"><strong><%= @player.web_name %></strong> <%= @player.past_ownership_stats[@gameweek.to_s] %>%</p>
  </div>
</div>
